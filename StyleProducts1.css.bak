body {
    
}
* {
    padding: 0;
    margin: 0;
    text-decoration: none;
    list-style: none;
    box-sizing: border-box;

}
nav {
    background-color: white;
    height: 12vh;
    width: 100vw;
    position: fixed;
    z-index: 12;
}


.logoim {
    height: 8vh;
    width: 80vw;
    margin-top: 2.5vh;
    object-fit: fill;
    margin-left:3vw;
    object-position: -24px 0;
}

nav ul{
    position:fixed;
    margin-top:0vh;
    left:-100%;
    background-color: white;
    height:100vh;
    width:100vw;
    display:block;
    text-align:center;
	padding-right:0%;

}
nav ul li{
    margin:15% 0;

}
    nav ul li a {
        color: black;
        text-decoration: none;
        font-size: 1.1rem;
        
    }
nav ul li a.active, nav ul li a:hover {
    color:#00630f;
	background:white;
	font-weight:600;
    font-size:1.3rem;
	
}
nav .menubtn i{
    color:black;
    font-size:1.8rem;
    cursor:pointer;
    display:block;
    float:right;
    padding-right:5%;
    padding-top:4.4vh;
}
#click{
    display:none;
}
#click:checked~ul{
    left:0;
    transition: all 0.4s ease;
}


