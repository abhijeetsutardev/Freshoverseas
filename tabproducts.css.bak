body {
    
}
* {
    padding: 0;
    margin: 0;
    text-decoration: none;
    list-style: none;
    box-sizing: border-box;
}
nav {
    background-color: white;
    height: 15.8%;
    width: 100%;
    position: fixed;
    z-index: 12;
}


.logoim {
    height: 76%;
    width: 40%;
    margin-top: 10px;
    object-fit: fill;
    margin-left:2%;
    object-position: -30px 0;
}

nav ul{
    position:fixed;
    margin-top:0vh;
    left:-100%;
    background-color: white;
    height:100vh;
    width:100%;
    display:block;
    text-align:center;

}
nav ul li{
    margin:40px 0;
}
    nav ul li a {
        color: black;
        text-decoration: none;
        font-size: 1.1rem;
        
    }
nav ul li a.active, nav ul li a:hover {
    color:#0058f0;
	background:white;
	font-weight:600;
    font-size:1.3rem;
	
}
nav .menubtn i{
    color:black;
    font-size:1.6rem;
    cursor:pointer;
    display:block;
    float:right;
    padding-right:5%;
    padding-top:4%;
}
#click{
    display:none;
}
#click:checked~ul{
    left:0;
    transition: all 0.4s ease;
}


